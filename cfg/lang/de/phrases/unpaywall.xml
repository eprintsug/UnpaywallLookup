<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE phrases SYSTEM "entities.dtd">

<epp:phrases xmlns="http://www.w3.org/1999/xhtml" xmlns:epp="http://eprints.org/ep3/phrase" xmlns:epc="http://eprints.org/ep3/control">

  <!-- Phrases for Unpaywall Upload InputForm Component -->
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:help">Diese Komponente benutzt den Unpaywall-Dienst,
    um mögliche OA Dokumente für diese Publikation im Web aufzufinden.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:version_title">PDF (<epc:pin name="version"/>) 
    verfügbar</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:default_title">Unpaywall-Dienst </epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:oa_publisher">Eine PDF-Datei der Publikation 
    (<epc:pin name="version"/>) ist Open Access auf der Verlagswebsite unter folgender URL erhältlich: 
    <epc:pin name="url"/>.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:oa_publisher_bronze">Eine PDF-Datei der Publikation 
    (<epc:pin name="version"/>) ist frei auf der Verlagswebsite unter folgender URL erhältlich: 
    <epc:pin name="url"/>. Nachdem Sie es in ZORA hochgeladen haben, überprüft die ZORA-Redaktion, ob das Dokument 
    Open Access ist. Falls nicht, wird es nur für registrierte Benutzer zugänglich gemacht.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:oa_repository">Eine PDF-Datei der Publikation 
    (<epc:pin name="version"/>) ist Open Access bei einem <epc:pin name="host_type"/> unter folgender URL 
    erhältlich: <epc:pin name="url"/>.<br/>
    Bitte prüfen Sie die PDF-Datei zunächst auf Deckblätter oder Wasserzeichen, bevor Sie sich entscheiden, 
    sie in ZORA hochzuladen.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:oa_license">Die Publikation ist zugänglich unter 
    einer <epc:pin name="license"/> Lizenz.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:no_submitted_version">Aufgrund der 
    <a href="https://www.hbz.uzh.ch/de/open-access-und-open-science/zora/zora-policy.html" target="_blank">ZORA Policy</a>
    können eingereichte Manuskripte (Preprints) gegenwärtig nicht akzeptiert werden.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:not_oa">Gemäss Unpaywall gibt es keine Open Access 
    Publikation.</epp:phrase>

  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:repository">Repositorium</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:publisher">Verlag</epp:phrase>

  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:download">Download</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:get_pdf">Zugang zum PDF</epp:phrase>

  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:publishedVersion">Publizierte Version</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:acceptedVersion">Akzeptiertes Manuskript</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:submittedVersion">Eingereichtes Manuskript (Preprint)</epp:phrase>

  <!-- License phrases -->
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by">CC BY</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by-nc">CC BY-NC</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by-sa">CC BY-SA</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by-nd-nc">CC BY-ND-NC</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by-nc-nd">CC BY-NC-ND</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_cc-by-nc-sa">CC BY-NC-SA</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_implied-oa">OA (gefolgert von Unpaywall)</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:license_publisher">Verlag</epp:phrase>

  <!-- Errors -->
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:title_doierror">Unpaywall-Dienst: DOI fehlt oder 
    ist ungültig</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:title_error">Unpaywall-Dienst: Ein Fehler ist
    aufgetreten</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:not_oa_title">Unpaywall-Dienst: Kein Open Access PDF 
    gefunden</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:no_doi">Der ZORA-Eintrag besitzt keinen DOI.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:invalid_doi">Der DOI <epc:pin name="doi"/> 
    des ZORA-Eintrags ist ungültig. Bitte korrigieren Sie ihn. Ein DOI muss mit "10." beginnen</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:title_unpaywall_unavailable">Unpaywall nicht verfügbar</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:unable_to_reach_unpaywall">Der Unpaywall-Dienst ist
    momentan nicht verfügbar.</epp:phrase>
  <epp:phrase id="Plugin/InputForm/Component/Upload_Unpaywall:unpaywall_error">Der Unpaywall-Dienst meldet einen
    Fehler: <epc:pin name="message"/></epp:phrase>

 <!-- Phrases for Unpaywall Surround help toggle -->
 <epp:phrase id="Plugin/InputForm/Surround/Unpaywall:show_help">
    <epc:pin name="link">
      <span class="fa fa-question-circle fa-lg" aria-hidden="true"></span>
      <span class="sr-only">Hilfe</span>
    </epc:pin>
  </epp:phrase>
  <epp:phrase id="Plugin/InputForm/Surround/Unpaywall:hide_help">
    <epc:pin name="link">
      <span class="fa fa-minus-circle fa-lg" aria-hidden="true"></span>
      <span class="sr-only">Hilfe schliessen</span>
    </epc:pin>
  </epp:phrase>

</epp:phrases>
